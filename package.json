{
  "name": "website-evidence-collector",
  "version": "2.1.1",
  "description": "automation of website evidence collection for storage and transfer of personal data based on puppeteer",
  "homepage": "https://edps.europa.eu/press-publications/edps-inspection-software_en",
  "repository": {
    "type": "git",
    "url": "https://github.com/EU-EDPS/website-evidence-collector.git"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@cliqz/adblocker-puppeteer": "^1.25.2",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.7.5",
    "@types/pug": "^2.0.10",
    "body-parser": "^1.20.3",
    "content-security-policy-parser": "^0.6.0",
    "copyfiles": "^2.4.1",
    "express": "^4.21.0",
    "fs-extra": "^11.1.0",
    "git-describe": "^4.0.4",
    "github-markdown-css": "^5.7.0",
    "got": "^11.8.5",
    "html-to-docx": "^1.8.0",
    "js-yaml": "^4.1.0",
    "js-yaml-js-types": "^1.0.0",
    "jstransformer-marked": "1.4.0",
    "lodash": "^4.17.21",
    "marked-smartypants": "^1.0.2",
    "pug": "^3.0.2",
    "puppeteer": "^22.12.1",
    "puppeteer-har": "^1.1.2",
    "rimraf": "^6.0.1",
    "seedrandom": "^3.0.5",
    "serve-static": "^1.16.2",
    "stacktrace-js": "^2.0.2",
    "tmp": "^0.2.1",
    "tough-cookie": "^4.1.3",
    "winston": "^3.3.3",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.10",
    "@types/puppeteer-har": "^1.1.0",
    "@types/validator": "^13.12.2",
    "@types/yargs": "^17.0.33",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.5.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "marked": "8.0.1",
    "puppeteer-har": {
      "chrome-har": "^0.13.1"
    }
  },
  "scripts": {
    "clean": "rimraf built/",
    "copy-assets": "copyfiles --follow src/assets/frontend/**/* src/assets/frontend/* src/assets/*  package.json built/",
    "start": "node built/bin/website-evidence-collector.js",
    "test": "jest --detectOpenHandles",
    "build-frontend": "cd frontend && npm run build",
    "build-ts": "tsc -p .",
    "setup": "npm run clean && npm run build-frontend && npm run build-ts && npm run copy-assets"
  },
  "bin": {
    "website-evidence-collector": "bin/website-evidence-collector.ts"
  },
  "keywords": [
    "privacy",
    "tracking",
    "website",
    "data",
    "protection",
    "personal",
    "data",
    "GDPR",
    "puppeteer"
  ],
